{
  "version": 1,
  "cli": {
    "analytics": false,
    "packageManager": "yarn",
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "fe-photos",
  "projects": {
    "fe-photos": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "inlineTemplate": true,
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "apps/fe/photos",
      "sourceRoot": "apps/fe/photos/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/fe/photos",
            "index": "apps/fe/photos/src/index.html",
            "main": "apps/fe/photos/src/main.ts",
            "polyfills": "apps/fe/photos/src/polyfills.ts",
            "tsConfig": "apps/fe/photos/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/fe/photos/src/favicon.ico",
              "apps/fe/photos/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "apps/fe/photos/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/fe/photos/src/environments/environment.ts",
                  "with": "apps/fe/photos/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "fe-photos:build",
            "proxyConfig": "apps/fe/photos/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "fe-photos:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "fe-photos:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/fe/photos/src/test.ts",
            "polyfills": "apps/fe/photos/src/polyfills.ts",
            "tsConfig": "apps/fe/photos/tsconfig.spec.json",
            "karmaConfig": "apps/fe/photos/karma.conf.js",
            "assets": [
              "apps/fe/photos/src/favicon.ico",
              "apps/fe/photos/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "apps/fe/photos/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/fe/photos/tsconfig.app.json",
              "apps/fe/photos/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "fe-photos:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "fe-photos:serve:production"
            }
          }
        }
      }
    },
    "fe-photos-e2e": {
      "root": "apps/fe/photos-e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/fe/photos-e2e/protractor.conf.js",
            "devServerTarget": "fe-photos:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "fe-photos:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/fe/photos-e2e/tsconfig.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "api-photos": {
      "root": "apps/api/photos",
      "sourceRoot": "apps/api/photos/src",
      "projectType": "application",
      "prefix": "api-photos",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/api/photos",
            "main": "apps/api/photos/src/main.ts",
            "tsConfig": "apps/api/photos/tsconfig.app.json",
            "assets": ["apps/api/photos/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/photos/src/environments/environment.ts",
                  "with": "apps/api/photos/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api-photos:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/api/photos/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/api/photos"],
          "options": {
            "jestConfig": "apps/api/photos/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
